<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net10.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>app_icon.ico</ApplicationIcon>
    <AssemblyName>InstAnalytics</AssemblyName>
    <RootNamespace>InstAnalytics</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <Resource Include="app_icon.ico" />
    <Resource Include="info_icon.png" />
    <Resource Include="export_instructions.jpg" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="ScottPlot.WPF" Version="5.1.57" />
  </ItemGroup>

</Project>
